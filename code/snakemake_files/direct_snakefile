
rule gen_quh_chunk_direct:
    input:
        evdf=config["EVD_DIR"]+"{snpset}_{dataset}_{bdf}_hapmap.h5",
        traitf=config["KG_DIRECTORY"]+"RSSp_sim_gwas_pheno/{snpset}_{dataset}_{scen}_trait.h5"
    params:
        pvv="{pvv}",
        svd="{svd}"
    output:
        quhf=config["KG_DIRECTORY"]+"RSSp_genome_direct_quh_chunk/{snpset}_{dataset}_{dataset}_{bdf}_{scen}_{pvv}_{svd}.h5"
    script:
        "../scripts/gen_quh_chunk_direct.R"


        
